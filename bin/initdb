#!/usr/bin/env sh

# Use this command on terminal to login to the database
# psql --host="${POSTGRES_HOST}" --username="${POSTGRES_USER}" --dbname="${POSTGRES_DB}" --file=/app/db/init.sql --no-password

psql \
  --host="${POSTGRES_HOST}" \
  --username="${POSTGRES_USER}" \
  --dbname="${POSTGRES_DB}" \
  --file=/app/db/init.sql \
  --no-password
